{
  "name": "ol3-lab",
  "version": "5.1.3",
  "description": "OpenLayers Lab",
  "main": "index.js",
  "scripts": {
    "update:all": "npm install ol3-fun ol3-grid ol3-input ol3-layerswitcher ol3-panzoom ol3-popup ol3-symbolizer ol3-draw ol3-search -D",
    "deploy:ol": "cp -r ./node_modules/ol/ol.css ./static/ol.css",
    "deploy:mocha": "cp -r ./node_modules/mocha/mocha.* ./static/",
    "deploy:chai": "cp -r ./node_modules/chai/chai.js ./static/",
    "deploy:requirejs": "cp -r ./node_modules/requirejs/require.js ./static/require.js",
    "deploy:css-layerswitcher": "cp -r ./node_modules/ol3-layerswitcher/ol3-layerswitcher/css/** ./static/css/",
    "deploy:css-panzoom": "cp -r ./node_modules/ol3-panzoom/ol3-panzoom/resources/** ./static/css/",
    "deploy:css-popup": "cp -r ./node_modules/ol3-popup/ol3-popup/css/** ./static/css/",
    "clean": "rm built/** -rf",
    "build:index": "tsc -p tsconfig.json --outFile ./built/index.js",
    "dev": "npm run build && start npm run live && npm run build -- -w",
    "live": "live-server --port=5000 --wait=200 --watch=./built/index.js --open=./loaders/tests.html --browser=CHROME",
    "build-for-loaders": "tsc -p ./loaders/tsconfig.json",
    "before-push": "npm run clean && npm run build && npm run build-for-loaders && npm run live",
    "test": "npm run live",
    "build:all": "npm run build-for-loaders && npm run build",
    "start": "parcel index.html",
    "build": "parcel build --public-url . .index.html",
    "rollup": "rollup ./built/index.js --file ./built/bundle.js --format iife",
    "proxy": "cd ../reverse-proxy-cache && start npm run server"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "mocha": "^8.1.2",
    "npm-run-all": "^4.1.5",
    "ol": "^6.4.3",
    "parcel-bundler": "^1.12.4",
    "requirejs": "^2.3.6",
    "rollup": "^2.26.5",
    "typescript": "^4.0.2"
  }
}
