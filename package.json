{
	"name": "ol3-lab",
	"version": "5.1.3",
	"description": "OpenLayers Lab",
	"main": "index.js",
	"scripts": {
		"clean": "rm built/** -rf",
		"update": "npm install ol3-draw ol3-fun ol3-grid ol3-input ol3-layerswitcher ol3-panzoom ol3-popup ol3-search ol3-symbolizer",
		"uglify": "pushd built && uglifyjs index.js --source-map \"filename='index.js.map' content='index.js.map'\" --output index.js --compress --mangle && popd",
		"build": "tsc -p tsconfig.json --outFile ./built/index.js",
		"css-layerswitcher": "cp -r ./node_modules/ol3-layerswitcher/ol3-layerswitcher/css/** ./static/css/",
		"css-panzoom": "cp -r ./node_modules/ol3-panzoom/ol3-panzoom/resources/** ./static/css/",
		"css-popup": "cp -r ./node_modules/ol3-popup/ol3-popup/css/** ./static/css/",
		"css": "npm run css-layerswitcher && npm run css-popup && npm run css-panzoom",
		"dev": "npm run build && start npm run live && npm run build -- -w",
		"live": "live-server --port=5000 --wait=200 --watch=./built/index.js --open=./loaders/tests.html --browser=CHROME",
		"build-for-loaders": "tsc -p ./loaders/tsconfig.json",
		"before-push": "npm run clean && npm run build && npm run build-for-loaders && npm run live",
		"test": "npm run live",
		"install": "npm run build-for-loaders && npm run build"
	},
	"devDependencies": {
		"@types/arcgis-js-api": "^4.8.1",
		"@types/jquery": "^3.3.6",
		"@types/mocha": "^5.2.5",
		"@types/openlayers": "^4.6.8",
		"@types/requirejs": "^2.1.31",
		"css-element-queries": "https://github.com/marcj/css-element-queries/tarball/master",
		"jquery": "^3.3.1",
		"live-server": "^1.2.0",
		"mocha": "^5.2.0",
		"node-sass": "^4.9.3",
		"ol3-draw": "https://github.com/ca0v/ol3-draw/tarball/v5.1.3",
		"ol3-fun": "https://github.com/ca0v/ol3-fun/tarball/v5.1.3",
		"ol3-grid": "https://github.com/ca0v/ol3-grid/tarball/v5.1.3",
		"ol3-input": "https://github.com/ca0v/ol3-input/tarball/v5.1.3",
		"ol3-layerswitcher": "https://github.com/ca0v/ol3-layerswitcher/tarball/v5.1.3",
		"ol3-panzoom": "https://github.com/ca0v/ol3-panzoom/tarball/v5.1.3",
		"ol3-popup": "https://github.com/ca0v/ol3-popup/tarball/v5.1.3",
		"ol3-search": "https://github.com/ca0v/ol3-search/tarball/v5.1.3",
		"ol3-symbolizer": "https://github.com/ca0v/ol3-symbolizer/tarball/v5.1.3",
		"requirejs": "^2.3.6",
		"typescript": "^3.1.0-dev.20180912",
		"uglify-js": "^3.3.20"
	},
	"dependencies": {}
}
