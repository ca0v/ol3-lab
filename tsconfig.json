{
	"compilerOptions": {
		"removeComments": true,
		"declaration": false,
		"module": "amd",
		"target": "es5",
		"noImplicitAny": false,
		"sourceMap": true,
		"inlineSourceMap": false,
		"inlineSources": true,
		"outFile": "./built/index.js",
		"moduleResolution": "classic",
		"baseUrl": "./node_modules",
		"rootDir": "./",
		"lib": ["esnext", "dom"]
	},
	"include": ["ol3-lab/**/*.ts"],
	"exclude": ["ol3-lab/tests/data/routes", "ol3-lab/tests/routing.ts"]
}
