{
	"compilerOptions": {
		"target": "ES2015",
		"module": "AMD",
		"allowJs": true,
		"checkJs": false,
		"noEmitOnError": true,
		"declaration": false,
		"incremental": true,
		"skipDefaultLibCheck": true,
		"removeComments": true,
		"noImplicitAny": false,
		"sourceMap": true,
		"inlineSourceMap": false,
		"inlineSources": false,
		"rootDir": "./",
		"outFile": "./built/index.js",
		"lib": [
			"ES2015",
			"dom"
		],
		"baseUrl": "./",
		"paths": {
			"@ol/*": [
				"./node_modules/ol/src/*"
			]
		},
		"moduleResolution": "classic",
	},
	"files": [
		"./index.ts"
	]
}